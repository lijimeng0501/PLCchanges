</Line>
<Line Number="142">
<![CDATA[  Run_M:=0;]]>
</Line>
<Line Number="143">
<![CDATA[END_IF;]]>
</Line>
<Line Number="144">
<![CDATA[IF Run_H=32767 THEN]]>
</Line>
<Line Number="145">
<![CDATA[  Run_H:=0;]]>
</Line>
<Line Number="146">
<![CDATA[END_IF;]]>
</Line>
<Line Number="147">
<![CDATA[//清除记录]]>
</Line>
<Line Number="148">
<![CDATA[IF Total_Clear OR CmdNo=10 THEN]]>
</Line>
<Line Number="149">
